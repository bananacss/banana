{
  "name": "bananacss",
  "description": "The Brazilian CSS superset.",
  "version": "0.8.0",
  "author": {
    "name": "afonsopacifer",
    "email": "afonsopacifer@live.com"
  },
  "bugs": {
    "url": "https://github.com/bananacss/bananacss/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bananacss/bananacss"
  },
  "keywords": [
    "css",
    "parser",
    "style",
    "preprocessor",
    "stylesheets",
    "superset",
    "language"
  ],
  "license": "MIT",
  "preferGlobal": true,
  "main": "src/banana.js",
  "bin": {
    "banana": "bin/index.js"
  },
  "dependencies": {
    "chokidar": "^1.6.1",
    "commander": "^2.9.0",
    "css": "^2.2.1",
    "css-ast-iterations": "^0.5.0"
  },
  "scripts": {
    "test": "mocha test/index.js",
    "cover": "istanbul cover _mocha -- ./test/* --recursive",
    "coveralls": "npm run cover -- --report lcovonly && cat ./coverage/lcov.info | coveralls",
    "eslint": "eslint src/* src/core/* src/helpers/* bin/* test/*",
    "jsdocs": "jsdoc bin/*.js src/*.js src/core/*.js src/helpers/*.js"
  },
  "devDependencies": {
    "coveralls": "^2.13.1",
    "eslint": "^3.19.0",
    "istanbul": "^0.4.5",
    "jsdoc": "^3.4.3",
    "mocha": "^3.3.0",
    "mocha-lcov-reporter": "^1.3.0"
  }
}
